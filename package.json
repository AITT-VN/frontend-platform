{
  "name": "@edx/frontend-i18n",
  "version": "1.0.0-semantically-released",
  "description": "Frontend i18n client code for managing internationalization and localization.",
  "main": "dist/main.js",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "NODE_ENV=production BABEL_ENV=production webpack --config webpack.config.js",
    "gc": "commit",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "lint": "eslint --ext .js --ext .jsx .",
    "is-es5": "es-check es5 ./dist/*.js",
    "precommit": "npm run lint",
    "prepublishOnly": "npm run build",
    "semantic-release": "semantic-release",
    "test": "jest --coverage",
    "travis-deploy-once": "travis-deploy-once"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/edx/frontend-i18n.git"
  },
  "author": "edX",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/edx/frontend-i18n/issues"
  },
  "homepage": "https://github.com/edx/frontend-i18n#readme",
  "devDependencies": {
    "@commitlint/cli": "^7.1.2",
    "@commitlint/config-angular": "^6.0.2",
    "@commitlint/prompt": "^6.0.2",
    "@commitlint/prompt-cli": "^6.0.2",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "coveralls": "^3.0.0",
    "es-check": "^5.0.0",
    "eslint": "^5.2.0",
    "eslint-config-edx": "^4.0.4",
    "husky": "^0.14.3",
    "jest": "23.6.0",
    "semantic-release": "^15.1.7",
    "source-map-loader": "^0.2.1",
    "travis-deploy-once": "^5.0.0",
    "uglifyjs-webpack-plugin": "^1.1.8",
    "webpack": "^4.16.2",
    "webpack-cli": "^3.1.0"
  },
  "dependencies": {
    "@cospired/i18n-iso-languages": "^2.0.2",
    "@edx/frontend-logging": "^2.0.1",
    "glob": "^7.1.4",
    "i18n-iso-countries": "^4.0.0",
    "iso-countries-languages": "^0.2.1",
    "react": "^16.8.6",
    "react-intl": "^2.9.0",
    "universal-cookie": "^4.0.0"
  },
  "peerDependencies": {
    "@edx/frontend-logging": "^2.0.1",
    "react": "^16.8.6"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/index.js",
      "!**/node_modules/**"
    ]
  }
}
